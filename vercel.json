{
  "version": 2,
  "builds": [{ "src": "dist/**", "use": "@vercel/static" }],
  "routes": [
    {
      "src": "/(.*\\.(?:js|css|map|json|txt|xml))",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(img|fonts)/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/_.*",
      "status": 200,
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
